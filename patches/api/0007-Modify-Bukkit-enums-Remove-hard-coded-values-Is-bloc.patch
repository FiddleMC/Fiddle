+    private static boolean isBlockIsSet = false; // Fiddle - remove hard-coded Bukkit values - Material - is block
+
     /**
      * Checks if this Material is a placable block
      *
      * @return true if this material is a block
      */
     public boolean isBlock() {
-    // Paper start - cache isBlock
+        // Fiddle start - remove hard-coded Bukkit values - Material - is block
+        if (!isBlockIsSet) {
+            for (Material material : values) {
+                material.isBlock = material.isLegacy() ? material.isBlockLegacy() : Bukkit.getUnsafe().isBlock(material);
+            }
+            isBlockIsSet = true;
+        }
