index 0000000000000000000000000000000000000000..1d58825ec41b2d67f68a09c4d67628892abcf04f
--- /dev/null
+++ b/src/main/java/org/fiddlemc/fiddle/pack/reader/PackParseException.java
@@ -0,0 +1,18 @@
+// Fiddle - read packs - exception class
+
+package org.fiddlemc.fiddle.pack.reader;
+
+/**
+ * A checked exception that occurred while parsing part of a pack.
+ */
+public class PackParseException extends Exception {
+
+    public PackParseException(String message) {
+        super(message);
+    }
+
+    public PackParseException(String message, Throwable cause) {
+        super(message, cause);
+    }
+
+}
