 @SuppressWarnings("deprecation")
 public final class CraftMagicNumbers implements UnsafeValues {
@@ -624,6 +625,22 @@ public final class CraftMagicNumbers implements UnsafeValues {
     }
     // Paper end
 
+    // Fiddle start - load packs - blocks - flag for added blocks - API
+    @Override
+    public boolean isBlockAddedByFiddle(@NotNull Material material) {
+        var block = this.getBlock(material);
+        return block != null && block.isAddedByFiddle;
+    }
+    // Fiddle end - load packs - blocks - flag for added blocks - API
+
+    // Fiddle start - load packs - blocks - flag for added items - API
+    @Override
+    public boolean isItemAddedByFiddle(@NotNull Material material) {
+        var item = this.getItem(material);
+        return item != null && item.isAddedByFiddle;
+    }
+    // Fiddle end - load packs - blocks - flag for added items - API
+
