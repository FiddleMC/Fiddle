index 0000000000000000000000000000000000000000..ad8ba3247d2113773f161ee89a1005413bcea4d7
--- /dev/null
+++ b/src/main/java/org/fiddlemc/fiddle/pack/read/PackParseException.java
@@ -0,0 +1,18 @@
+// Fiddle - read packs - exception class
+
+package org.fiddlemc.fiddle.pack.read;
+
+/**
+ * A checked exception that occurred while parsing part of a pack.
+ */
+public class PackParseException extends Exception {
+
+    public PackParseException(String message) {
+        super(message);
+    }
+
+    public PackParseException(String message, Throwable cause) {
+        super(message, cause);
+    }
+
+}
